{
    "name": "aeviiq/form-flow",
    "type": "library",
    "description": "A library that provides a basic form flow implementation.",
    "keywords": [
        "form-flow",
        "flow",
        "form"
    ],
    "homepage": "https://github.com/aeviiq/form-flow",
    "license": "MIT",
    "authors": [
        {
            "name": "Aeviiq Form Flow contributors",
            "homepage": "https://github.com/aeviiq/form-flow/graphs/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "Aeviiq\\FormFlow\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aeviiq\\FormFlow\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2",
        "aeviiq/collection": "^3.2",
        "aeviiq/storage-manager": "^3.0",
        "aeviiq/enum": "^2.0",
        "symfony/form": "^4.3|^5.0",
        "symfony/event-dispatcher": "^4.3|^5.0",
        "symfony/http-foundation": "^4.3|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2"
    }
}
